// prisma/schema.prisma//
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LogEvent {
  id        String   @id @default(cuid())
  type      String
  ts        DateTime
  sessionId String?
  userId    String?
  url       String?
  referrer  String?
  ua        String?
  lang      String?
  props     Json
}
